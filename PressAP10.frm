Attribute VB_Name = "Press"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim YY As Integer
Dim XX As Integer
Private Sub Command1_Click()
        If PSN = 1 Then YY = 0
        If PSN = 8 Then
            MsgBox "Can't make another Segment"
            GoTo 100
        End If
        If PRate(PSN) = 0 Then
            MsgBox "Rate of ZERO not allowed"
            Text7.Text = ""
            Text7.SetFocus
            GoTo 100
        End If
        Text4(PSN).Text = "Segment"
        Label1(PSN).Caption = PSN
        Text10(0 + YY).Text = Str$(PStart(PSN))
        Text10(1 + YY).Text = Str$(PEnd(PSN))
        Text10(2 + YY).Text = Str$(PRate(PSN))
        YY = YY + 3

    PSN = Val(Label1(PSN).Caption) + 1
    Label1(0).Caption = PSN
    Text5.SetFocus
    Text5.Text = ""
    Text6.Text = ""
    Text7.Text = ""
100
End Sub
Private Sub Option1_Click()
    'Debug.Print "From Pressure form"
    'For XX = 1 To PSN - 1
    '    Debug.Print XX, PStart(XX), PEnd(XX), PRate(XX)
   ' Next XX
    PSN = PSN - 1
    MaxPSN = PSN
    StartPress = 1
    Unload Press
    ReDo = 1                'replot after exit
End Sub
Private Sub Option2_Click()
    YY = 0
    For XX = 1 To PSN - 1
        Text4(XX).Text = ""
        Label1(XX).Caption = ""
        PStart(XX) = "0"
        Text10(0 + YY).Text = ""
        PEnd(XX) = "0"
        Text10(1 + YY).Text = ""
        PEnd(XX) = "0"
        Text10(2 + YY).Text = ""
        YY = YY + 3
    Next XX
    Label1(0).Caption = "1"
    PSN = 0
    YY = 0
    Text5.SetFocus
    Option2.Value = False           'clear selection dot
End Sub
Private Sub Text5_KeyUP(KeyCode As Integer, Shift As Integer)
    PSN = Val(Label1(0).Caption)
    PStart(PSN) = Val(Text5.Text)
    If KeyCode = 13 Then Text6.SetFocus
End Sub
Private Sub Text6_KeyUP(KeyCode As Integer, Shift As Integer)
    PEnd(PSN) = Val(Text6.Text)
    If KeyCode = 13 Then Text7.SetFocus
End Sub
Private Sub Text7_KeyUP(KeyCode As Integer, Shift As Integer)
    PRate(PSN) = Val(Text7.Text)
    If KeyCode = 13 Then
        If PSN = 1 Then YY = 0
            If PSN = 8 Then
                MsgBox "Can't make another Segment"
                GoTo 200
            End If
            If PRate(PSN) = 0 Then
                MsgBox "Rate of ZERO not allowed"
                Text7.Text = ""
                Text7.SetFocus
                GoTo 200
            End If
            Text4(PSN).Text = "Segment"
            Label1(PSN).Caption = PSN
            Text10(0 + YY).Text = Str$(PStart(PSN))
            Text10(1 + YY).Text = Str$(PEnd(PSN))
            Text10(2 + YY).Text = Str$(PRate(PSN))
            YY = YY + 3
        PSN = Val(Label1(PSN).Caption) + 1
        Label1(0).Caption = PSN
        Text5.SetFocus
        Text5.Text = ""
        Text6.Text = ""
        Text7.Text = ""
200
    End If
End Sub
